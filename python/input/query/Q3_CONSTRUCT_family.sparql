PREFIX : <http://semantics.id/ns/example/film#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

CONSTRUCT {
  ?film rdf:type :Film ;
    rdfs:label ?film_title ;
    rdfs:comment "Constructed Knowledge" .
}
WHERE {
  ?film rdf:type :Film ;
  rdfs:label ?film_title .
}
